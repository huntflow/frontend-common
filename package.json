{
  "name": "huntkit",
  "version": "0.0.2",
  "scripts": {
    "serve": "vue-cli-service serve",
    "prepare-tokens": "style-dictionary build",
    "dev": "npm run prepare-tokens && vite build --watch",
    "build": "npm run prepare-tokens && vite build",
    "storybook": "npm run prepare-tokens && start-storybook -p 6006",
    "build-storybook": "npm run prepare-tokens && build-storybook"
  },
  "dependencies": {
    "nanoid": "^3.1.30",
    "vue": "^2.6.11"
  },
  "main": "dist/hunt-kit.cjs.js",
  "module": "dist/hunt-kit.es.js",
  "exports": {
    ".": {
      "import": "./dist/hunt-kit.es.js",
      "require": "./dist/hunt-kit.cjs.js"
    },
    "./hunt-kit.css": "./dist/style.css"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/jsdom": "^16.2.14",
    "typescript": "^4.5.4",
    "lodash": "^4.17.21",
    "@storybook/addon-essentials": "^6.2.9",
    "@vue/cli-plugin-babel": "^4.5.13",
    "core-js": "^3.6.5",
    "vite": "^2.6.3",
    "vite-plugin-vue2": "^1.8.2",
    "@babel/core": "^7.15.5",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/vue": "^6.3.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "style-dictionary": "^3.0.3",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.46.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "last 2 versions"
  ]
}
